{
  "Tokens": {
    "Issuer": "#{Token:ApplicationUrl}",
    "Key": "#{Token:SecurityKey}",
    "Security": "HS256",
    "ExpirationOffset": 2880
  },
  "Logging": {
    "Enabled": false,
    "Level": 1
  },
  "AllowedHosts": "*",
  "Redis": {
    "Host": "#{Redis:Host}",
    "Password": "#{Redis:Password}",
    "PageSize": 50000
  },
  "Database": {
    "Connection": "#{Database:ConnectionString}",
    "SetupScripts": [
      "Scripts\\Tables\\*.sql",
      "Scripts\\Procedures\\*.sql"
    ]
  }
}